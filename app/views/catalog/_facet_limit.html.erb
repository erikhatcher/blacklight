<ul>
  <% paginator =    
      Blacklight::Solr::FacetPaginator.new(facet_field.items,        
        :limit => facet_limit_for(facet_field.name))
  %>
  <% paginator.items.each do |item| -%>
    <li>
    <% if facet_in_params?( facet_field.name, item.value ) %>              
        <%= render_selected_facet_value(facet_field.name, item) %>              
      <% else %>              
        <%= render_facet_value(facet_field.name, item) %>              
    <% end -%>
    </li>
  <% end %>
  
  <% if(paginator.has_next?)  %>
    <li class="more_facets_link"><%= link_to('more Â»', params.merge(:id => facet_field.name, :action=>"facet"), :class => "more_facets_link") %></li>
  <% end %>
</ul>
   
